ENV=local
SECRET_KEY=
DATABASE_URL=
IBM_API_KEY=
IBM_BUCKET_ENDPOINT=
IBM_BUCKET=
IBM_BUCKET_TOKEN=
IBM_BUCKET_CRN=

# https://BUCKET-NAME.s3.<us-east | us-west | ...>.cloud-object-storage.appdomain.cloud/
# We suggest Bucket access policies to have "Public Access" enabled as an "Object Reader"
IBM_BUCKET_PUBLIC_ENDPOINT=

PROJECT_FOLDER=
MAINTENANCE=False
PASSWORD=pass
GOOGLE_MAP_STREET_API=
GOOGLE_MAP_API=
CACHE_LOCATION=

# CACHE LOCATION:
# c:/foo/bar for windows
# /var/tmp/django_cache for linux (make sure it has all permissions)

# PROJECT_FOLDER should not end with closing front or back slash '/' or '\' (keep it empty)
# e.g. /home/username/isac
# do not close isac

# ADMINS=Name:email,AnotherName:email
ADMINS=
EMAIL_HOST=smtp.gmail.com
EMAIL_USE_TLS=True
EMAIL_PORT=587
EMAIL_HOST_USER=
EMAIL_HOST_PASSWORD=